{
  "name": "@mystikonetwork/node",
  "version": "0.1.8",
  "description": "mystiko node package of napi wrapper",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/cjs/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mystikonetwork/mystiko-node.git",
    "directory": "node"
  },
  "author": "Mystiko Network",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "prettier": "run-p prettier:ts",
    "prettier:ts": "prettier . -w",
    "test": "jest --forceExit --coverage",
    "lint": "eslint . --ext .js,.ts"
  },
  "dependencies": {
    "@mystikonetwork/napi": "0.1.8",
    "@mystikonetwork/protos": "0.5.6"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "benny": "^3.7.1",
    "chalk": "^5.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.2"
}
