{
  "name": "@mystikonetwork/napi",
  "version": "0.1.6",
  "description": "mystiko napi package with napi-rs",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mystikonetwork/mystiko-node.git",
    "directory": "napi"
  },
  "author": "Mystiko Network",
  "license": "MIT",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api"
  ],
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "name": "mystiko-napi",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin"
      ]
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "bench": "node -r @swc-node/register benchmark/bench.ts",
    "build": "napi build --use-cross --platform --release",
    "build:debug": "napi build --platform",
    "prettier": "run-p prettier:ts prettier:rs prettier:toml",
    "prettier:ts": "prettier . -w",
    "prettier:toml": "taplo format",
    "prettier:rs": "cargo fmt",
    "lint:ts": "eslint .",
    "lint:rs-fmt": "cargo fmt -- --check",
    "lint:rs-clippy": "cargo clippy --all-targets --all-features -j 4 -- -D warnings",
    "lint": "run-p lint:ts lint:rs-fmt lint:rs-clippy",
    "prepublishOnly": "napi pre-publish",
    "test": "ava",
    "version": "napi version"
  },
  "devDependencies": {
    "@mystikonetwork/protos": "0.5.4",
    "@napi-rs/cli": "3.0.0-alpha.18",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.95",
    "@taplo/cli": "^0.5.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "ava": "^5.3.1",
    "benny": "^3.7.1",
    "chalk": "^5.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "ava": {
    "require": [
      "@swc-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "timeout": "2m",
    "workerThreads": false,
    "environmentVariables": {
      "TS_NODE_PROJECT": "./tsconfig.json"
    }
  },
  "packageManager": "yarn@4.0.2"
}
